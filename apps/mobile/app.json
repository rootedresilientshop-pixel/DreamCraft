{
  "expo": {
    "name": "DreamCraft",
    "slug": "dreamcraft",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000000",
    "newArchEnabled": true,
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.dreamcraft.app",
      "infoPlist": {
        "NSLocalNetworkUsageDescription": "This app uses local network to connect to development server",
        "NSBonjourServiceTypes": [
          "_http._tcp",
          "_https._tcp"
        ]
      }
    },
    "android": {
      "package": "com.dreamcraft.app",
      "backgroundColor": "#000000",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [],
    "scheme": [
      "dreamcraft",
      "com.dreamcraft.app"
    ],
    "extra": {
      "apiUrl": "https://api.render.com/api",
      "eas": {
        "projectId": "1e5195d7-d635-4c15-a5f8-21c96d4e9188"
      }
    },
    "owner": "gardners"
  }
}
