{
  "expo": {
    "name": "VentureLab",
    "slug": "venturelab",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000000",
    "newArchEnabled": true,
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.venturelab.app",
      "supportsTabletMode": true,
      "infoPlist": {
        "NSLocalNetworkUsageDescription": "This app uses local network to connect to development server",
        "NSBonjourServiceTypes": ["_http._tcp", "_https._tcp"]
      }
    },
    "android": {
      "package": "com.venturelab.app",
      "backgroundColor": "#000000",
      "permissions": ["INTERNET", "ACCESS_NETWORK_STATE"]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [],
    "scheme": [
      "dreamcraft",
      "com.venturelab.app"
    ],
    "extra": {
      "apiUrl": "https://api.render.com/api"
    }
  }
}
